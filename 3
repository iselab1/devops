cd../
echo stoping Batch Module...
appHome='pwd'
pid= $ (ps Aeflgrep 'pwd' | grep -v grep | awk '{print $2}')
kill -9$Spid
[echo = $ (ps - A.)]
echo Removing nohup.out file....
if [.f bin |nohup.out]
then
        vm -rf bin/nohup.out
        fi
        cd standalong/log/
        timeNow = '(date--date= 'now' + %Y -% m -%d_%H)'
        cd $ {appHome}/bin
        echo Restarting Application....
        nohup./standalone.sh - bmanagement = 127.0.0.1 - b0.0.0.0 &
        sleep 5
